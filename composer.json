{
    "name": "huangdijia/laravel-macro-ide-helper",
    "description": "Laravel IDE Helper, generates correct PHPDocs for all Macros method, to improve auto-completion.",
    "keywords": ["laravel", "autocomplete", "ide", "helper", "phpstorm", "netbeans", "sublime", "codeintel", "phpdoc"],
    "license": "MIT",
    "authors": [{
        "name": "Huangdijia",
        "email": "huangdijia@gmail.com"
    }],
    "require": {
        "php": ">=7",
        "illuminate/support": "^5.7|^6.0|^7.0|^8.0",
        "illuminate/console": "^5.7|^6.0|^7.0|^8.0",
        "illuminate/filesystem": "^5.7|^6.0|^7.0|^8.0",
        "barryvdh/reflection-docblock": "^2.0.6",
        "composer/composer": "^1.6"
    },
    "require-dev": {
        "illuminate/config": "^5.7|^6.0|^7.0|^8.0",
        "illuminate/view": "^5.7|^6.0|^7.0|^8.0",
        "phpro/grumphp": "^0.14",
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "^1.1",
        "squizlabs/php_codesniffer": "^3",
        "doctrine/dbal": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Huangdijia\\IdeHelper\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Huangdijia\\IdeHelper\\IdeHelperServiceProvider"
            ]
        }
    }
}